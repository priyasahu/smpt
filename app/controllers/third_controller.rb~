class ThirdController < ApplicationController
	
	layout 'ThirdPage'
  def wer
  	  
    @user = User.find(1)

    respond_to do |format|
      format.html # show.html.erb
      format.xml  { render :xml => @user }
    end
  
  end
  
  def cuttingedge
